<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Excel Viewer</title>
    <link rel="stylesheet" href="css/excel-view.css"> <!-- Include the generated CSS file -->
</head>

<body>
    <h1>Excel Viewer</h1>
    <table>
        <thead>
            <tr>
                <% for (let header of headers) { %>
                    <th>
                        <%= header %>
                    </th>
                    <% } %>
            </tr>
        </thead>
        <tbody>
            <% for (let row of rows) { %>
                <tr>
                    <% for (let cell of row) { %>
                        <td>
                            <%= cell %>
                        </td>
                        <% } %>
                </tr>
                <% } %>
        </tbody>
    </table>
</body>

</html>
